<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plot_spec>
    <connection>
        <host>HOST</host>
        <database>DB_NAME</database>
        <user>USER</user>
        <password>PASS</password>
        <management_system>mariadb</management_system>
    </connection>
    <rscript>/usr/local/R/bin/Rscript</rscript>
    <folders>
        <r_tmpl>R_TMPL</r_tmpl>
        <r_work>R_WORK</r_work>
        <plots>PLOTS</plots>
        <data>DATA</data>
        <scripts>SCRIPTS</scripts>
    </folders>
    <plot>
        <template>series_plot.R_tmpl</template>
        <dep>
            <dep1>
                <fcst_var name="WIND">
                    <stat>ME</stat>
                </fcst_var>
            </dep1>
            <dep2/>
        </dep>
        <series1>
            <field name="model">
                <val>MODEL_1_mean</val>
                <val>MODEL_1_mem01</val>
                <val>MODEL_1_mem02</val>
                <val>MODEL_1_mem03</val>
                <val>MODEL_1_mem04</val>
                <val>MODEL_1_mem05</val>
                <val>MODEL_1_mem06</val>
                <val>MODEL_1_mem07</val>
                <val>MODEL_1_mem08</val>
                <val>MODEL_1_mem09</val>
                <val>MODEL_1_mem10</val>
                <val>MODEL_2_mean</val>
                <val>MODEL_2_mem01</val>
                <val>MODEL_2_mem02</val>
                <val>MODEL_2_mem03</val>
                <val>MODEL_2_mem04</val>
                <val>MODEL_2_mem05</val>
                <val>MODEL_2_mem06</val>
                <val>MODEL_2_mem07</val>
                <val>MODEL_2_mem08</val>
                <val>MODEL_2_mem09</val>
                <val>MODEL_2_mem10</val>
            </field>
        </series1>
        <series2/>
        <plot_fix>
            <field equalize="false" name="vx_mask">
                <set name="vx_mask_0">
                    <val>CONUS</val>
                </set>
            </field>
            <field equalize="false" name="obtype">
                <set name="obtype_1">
                    <val>ADPSFC</val>
                </set>
            </field>
            <field equalize="false" name="fcst_lev">
                <set name="fcst_lev_2">
                    <val>Z10</val>
                </set>
            </field>
            <field equalize="false" name="fcst_init_beg">
                <set name="fcst_init_beg_3">
                    INIT_DATES
                </set>
            </field>
        </plot_fix>
        <plot_cond/>
        <indep equalize="false" name="fcst_lead">
            FHR_LEAD
        </indep>
        <agg_stat>
            <agg_sl1l2>true</agg_sl1l2>
            <boot_repl>1</boot_repl>
            <boot_random_seed/>
            <boot_ci>perc</boot_ci>
            <eveq_dis>false</eveq_dis>
            <cache_agg_stat>false</cache_agg_stat>
            <circular_block_bootstrap>true</circular_block_bootstrap>
        </agg_stat>
        <plot_stat>median</plot_stat>
        <tmpl>
            <data_file>TMPL_INFO_10mW_bias.data</data_file>
            <plot_file>TMPL_INFO_10mW_bias.png</plot_file>
            <r_file>TMPL_INFO_10mW_bias.R</r_file>
            <title>10-m Wind Speed Bias (m/s) TITLE</title>
            <x_label>Forecast Lead Time (h)</x_label>
            <y1_label>Bias</y1_label>
            <y2_label/>
            <caption/>
            <job_title>TMPL_INFO_10mW_bias</job_title>
            <keep_revisions>false</keep_revisions>
            <listdiffseries1>list()</listdiffseries1>
            <listdiffseries2>list()</listdiffseries2>
        </tmpl>
        <execution_type>Python</execution_type>
        <event_equal>false</event_equal>
        <vert_plot>false</vert_plot>
        <x_reverse>false</x_reverse>
        <num_stats>false</num_stats>
        <indy1_stag>false</indy1_stag>
